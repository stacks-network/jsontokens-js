{
  "name": "jsontokens",
  "version": "0.8.0",
  "description": "node.js library for encoding, decoding, and verifying JSON Web Tokens (JWTs)",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets env src -d lib",
    "test": "eslint src/ && npm run compile && nyc --reporter=text node lib/test/index.js",
    "prepublish": "npm run compile",
    "browserify-app": "npm run compile && browserify lib/test/browserifyApp.js -o lib/test/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockstack/jsontokens-js.git"
  },
  "keywords": [
    "jwt",
    "json",
    "web",
    "token",
    "encode",
    "decode",
    "verify",
    "ecdsa",
    "secp256k1",
    "ec",
    "elliptic",
    "curve",
    "signature",
    "sign"
  ],
  "author": "Blockstack PBC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockstack/jsontokens-js/issues"
  },
  "homepage": "https://github.com/blockstack/jsontokens-js#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.1",
    "browserify-des": "^1.0.1",
    "eslint": "^5.6.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "nyc": "^13.1.0",
    "tape": "^4.2.0"
  },
  "dependencies": {
    "asn1.js": "^5.0.1",
    "base64url": "^3.0.1",
    "elliptic": "^6.4.1",
    "key-encoder": "^1.1.7",
    "validator": "^10.9.0"
  }
}
